<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/transparent"
    android:background="@drawable/upload_activity_round_dialog_custom"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/indicator"
        android:layout_marginTop="10dp"
        android:layout_gravity="center"
        ></ImageView>

    <LinearLayout
        android:id="@+id/setting_notice_off"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_notice_off"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 알람 끄기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/setting_notice_on"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_notice_on"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 알람 받기"
           android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/approval_line_color"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/setting_storage_on"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_storage_on"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 보관함에 넣기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/setting_storage_off"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_storage_off"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 보관함에서 빼기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/approval_line_color"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/setting_report_post"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_report_post"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 신고하기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/setting_edit_post"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_report_edit"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 수정하기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/approval_line_color"
        android:orientation="horizontal"></LinearLayout>


    <LinearLayout
        android:id="@+id/setting_report_user"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_report_user"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사용자 신고하기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/setting_remove_post"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        android:orientation="horizontal"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:weightSum="7">

        <ImageView
            android:layout_weight="1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:gravity="left"
            android:textSize="12dp"
            android:src="@drawable/community_setting_report_remove"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></ImageView>

        <TextView
            android:layout_weight="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이 보고서 삭제하기"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:fontFamily="@font/medium"></TextView>
    </LinearLayout>

</LinearLayout>